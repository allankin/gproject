<resources xmlns:android="http://schemas.android.com/apk/res/android">
	
    <style name="dial_liner">
        <item name="android:orientation">horizontal</item> 
        <item name="android:layout_width">fill_parent</item> 
        <item name="android:layout_height">60dp</item> 
    </style>
    
    <style name="dial_grid_btn">
        <item name="android:layout_weight">1</item> 
        <item name="android:layout_width">0dp</item> 
        <item name="android:layout_height">fill_parent</item> 
    </style>    
    
    <style name="Txl.Skin.Widget.Spinner" parent="android:Widget">
	     <item name="android:background">@drawable/select_dropdown</item>
	     <item name="android:clickable">true</item>
	</style>
	
    <style name="Txl.Skin.Default" parent="android:Theme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
</resources>